	file = {arXiv\:1408.2195 PDF:/home/mack/.zotero/zotero/aeihmoq1.default/zotero/storage/MVZF2QVV/Bouneffouf - 2014 - R-UCB a Contextual Bandit Algorithm for Risk-Awar.pdf:application/pdf;arXiv.org Snapshot:/home/mack/.zotero/zotero/aeihmoq1.default/zotero/storage/Z4SDQ4D6/1408.html:text/html}
