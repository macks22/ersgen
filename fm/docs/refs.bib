	keywords = {Computer Science - Data Structures and Algorithms, Computer Science - Social and Information Networks, Physics - Physics and Society},
	file = {arXiv\:1110.5813 PDF:/home/mack/.zotero/zotero/aeihmoq1.default/zotero/storage/C94FXJ64/Xie et al. - 2013 - Overlapping Community Detection in Networks the S.pdf:application/pdf;arXiv.org Snapshot:/home/mack/.zotero/zotero/aeihmoq1.default/zotero/storage/GD8CB9CB/1110.html:text/html}
