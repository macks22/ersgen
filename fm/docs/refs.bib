	file = {arXiv\:1401.3413 PDF:/home/mack/.zotero/zotero/aeihmoq1.default/zotero/storage/FHFD82HH/Saluja et al. - 2014 - Infinite Mixed Membership Matrix Factorization.pdf:application/pdf;arXiv.org Snapshot:/home/mack/.zotero/zotero/aeihmoq1.default/zotero/storage/I6DG6X2P/1401.html:text/html}
