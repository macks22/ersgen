	langid = {english},
	keywords = {Algebra, Bayesian Statistics, Comparative Analysis, Mathematics, Models, Prediction, Probability, Regression (Statistics), Teaching Methods, Tutors},
	file = {Full Text PDF:/home/mack/.zotero/zotero/aeihmoq1.default/zotero/storage/M9RVH2T6/Xu and Mostow - 2012 - Comparison of Methods to Trace Multiple Subskills.pdf:application/pdf;Snapshot:/home/mack/.zotero/zotero/aeihmoq1.default/zotero/storage/ZZ28ZVMU/eric.ed.gov.html:text/html}
