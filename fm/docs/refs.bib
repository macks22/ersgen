	date = {2007-10},
	keywords = {Accuracy, Data mining, Demography, Filtering, information filters, International collaboration, interpolation, interpolation weights, Large-scale systems, Motion pictures, Nearest neighbor searches, Netflix Cinematch recommender system, recommender systems, scalable collaborative filtering},
	file = {IEEE Xplore Abstract Record:/home/mack/.zotero/zotero/aeihmoq1.default/zotero/storage/8VUBHWA4/abs_all.html:text/html;IEEE Xplore Full Text PDF:/home/mack/.zotero/zotero/aeihmoq1.default/zotero/storage/DSXICT4Z/Bell and Koren - 2007 - Scalable Collaborative Filtering with Jointly Deri.pdf:application/pdf}
}

@incollection{singh_unified_2008,
	title = {A Unified View of Matrix Factorization Models},
