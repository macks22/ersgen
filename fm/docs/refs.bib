	location = {New York, {NY}, {USA}},
	title = {Factorization Meets the Neighborhood: A Multifaceted Collaborative Filtering Model},
	isbn = {978-1-60558-193-4},
	url = {http://doi.acm.org/10.1145/1401890.1401944},
