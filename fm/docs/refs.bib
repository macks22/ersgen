	keywords = {Algorithm Analysis and Problem Complexity, Artificial Intelligence (incl. Robotics), Database Management, Information Storage and Retrieval, Mathematical Logic and Formal Languages, Probability and Statistics in Computer Science},
	file = {Full Text PDF:/home/mack/.zotero/zotero/aeihmoq1.default/zotero/storage/7H89N76D/Singh and Gordon - 2008 - A Unified View of Matrix Factorization Models.pdf:application/pdf;Snapshot:/home/mack/.zotero/zotero/aeihmoq1.default/zotero/storage/C2M2Q7B9/10.html:text/html}
